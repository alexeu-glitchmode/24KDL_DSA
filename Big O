// Bài 1: Min Index
#include <stdio.h>
#include <stdlib.h>
#include <time.h>

void initializeArray (int arr[], int n, int min, int max) {
    arr[0] = min + rand() % (max - min + 1);
    for (int i = 0; i < n - 1; i++) {
        arr[i + 1] = arr[i] + abs(rand() % (max - arr[i] + 1));
    }
}

void insertArray (int arr[], int *n, int value, int index,  int *assignment) {
    *assignment = 0;
    if (index < 0 || index >= *n) return;
    (*n)++;
    for (int i = *n - 1; i > index; i--) {
        *assignment += 1;
        arr[i] = arr[i - 1];
    }
    arr[index] = value;
}

int main() {
    int arr[1000];
    int n;
    int comps;
    int k[] = {100,1000,10000};
    //Sinh mang ngau nhien
    time_t t;
    srand((unsigned)time(&t));

    for (int i = 0; i < 3; i++){
        int freTable[21] = {}; //tao bang tan so voi tat ca tan so ben trong la 0.
        for (int j = 0; j < k[i]; j++){
            n = 10;
            initializeArray(arr, n, -1000, 1000);
            int value = k[i];
            int index = rand() % (n - 1);
            insertArray(arr, &n, value, index, &comps);
            freTable[comps]++;
        }
        printf("k = %d; Mean = ",k[i]);
        float sum = 0;
        for (int l = 0; l < 21; l++)
            sum += l*freTable[l];
        sum /= k[i];
        printf("%f\n",sum);
    }
    return 0;
}


//Bài 2: Students' scores
#include <stdio.h>
#include <stdlib.h>
#include <time.h>

void initializeArray(float arr[], int n, int min, int max) {
    for (int i = 0; i < n; i++)
        arr[i] = min + (float)rand()/ RAND_MAX * (max - min);
}

void sortArray(float arr[], int n, int *comps){
    *comps = 0;
    for(int i=0; i<n-1; i++){
        int min_index = i;
        for(int j=i+1; j<n; j++){
            (*comps)++;
            if (arr[min_index] > arr[j]){
                min_index = j;
            }
        }
        float temp;
        temp = arr[min_index];
        arr[min_index] = arr[i];
        arr[i] = temp;
    }
}

int main() {
    float arr[1000];
    int n = 40;
    int comps;
    int k[] = {100,1000,10000};
    //Sinh mang ngau nhien
    time_t t;
    srand((unsigned)time(&t));
    
    for (int i = 0; i < 3; i++){
        int freTable[781] = {}; //tao bang tan so voi tat ca tan so ben trong la 0.
        for (int j = 0; j < k[i]; j++){
            initializeArray(arr, n, 0.0, 10.0);
            sortArray(arr, n, &comps);
            freTable[comps]++;
        }
        printf("k = %d; Mean = ",k[i]);
        float sum = 0;
        for (int l = 0; l < 781; l++)
            sum += (float)l*freTable[l];
        sum /= k[i];
        printf("%f\n",sum);
    }
    
}


//Bài 3
#include <stdio.h>
#include <stdlib.h>
#include <time.h>

// Sinh mảng ngẫu nhiên
void initializeArray(int arr[], int n, int min, int max) {
    for (int i = 0; i < n; i++)
        arr[i] = min + rand() % (max - min + 1);
}

void bubbleSort(int arr[], int n, int *comps) {
    *comps = 0;
    for (int i = 0; i < n - 1; i++) {
        for (int j = 0; j < n - i - 1; j++) {
            (*comps)++;         
            if (arr[j] > arr[j + 1]) {
                int tmp = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = tmp;
            }
        }
    }
}

int main() {
    int arr[1000];
    int comps;
    time_t t;
    srand((unsigned)time(&t));

    int k[] = {10, 20, 50, 100, 1000, 10000};
    int n = 20;

    printf("n = 20\n");
    for (int i = 0; i < 6; i++) {
        int freTable[1000] = {0};

        for (int j = 0; j < k[i]; j++) {
            initializeArray(arr, n, -1000, 1000);
            bubbleSort(arr, n, &comps);
            freTable[comps]++;
        }

        float sum = 0;
        for (int l = 0; l < 1000; l++)
            sum += l * freTable[l];

        printf("k = %d; Mean comps= %.2f\n",
               k[i], sum / k[i]);
    }

   
    printf("Khao sat theo kich thuoc n, k = 1000\n");
    int runs = 1000;
    for (n = 10; n <= 100; n += 10) {
        int total = 0;
        for (int i = 0; i < runs; i++) {
            initializeArray(arr, n, -1000, 1000);
            bubbleSort(arr, n, &comps);
            total += comps;
        }
        printf("n = %3d; Mean comps = %.2f\n",
               n, (float)total / runs);
    }
    
    printf("\nVới n=20, k thay đổi: Mean comps không thay đổi. Mean comps = n(n-1)/2");
    printf("\n Với n thay đổi, k = 1000: Mean comps tăng khi n tăng -> Mean comps phụ thuộc vào n. \nBig O = O(n^2)=");
    return 0;
}


// Bài 4
#include <stdio.h>
#include <stdlib.h>
#include <time.h>

void initializeArray(int arr[], int n, int min, int max) {
    for (int i = 0; i < n; i++)
        arr[i] = min + rand() % (max - min + 1);
}

// Xóa phần tử tại vị trí start (dịch trái)
void deleteArray(int arr[], int *n, int start, int *assignment) {
    for (int i = start; i < *n - 1; i++) {
        (*assignment)++;
        arr[i] = arr[i + 1];
    }
    (*n)--;
}

void sort(int arr[], int n, int *comps, int *assignment) {
    for (int i = 0; i < n - 1; i++) {
        for (int j = i + 1; j < n; j++) {
            (*comps)++;
            if (arr[i] > arr[j]) {
                (*assignment) += 3; // swap = 3 phép gán
                int temp = arr[i];
                arr[i] = arr[j];
                arr[j] = temp;
            }
        }
    }
}

// CASE A: duyệt và xóa trực tiếp
void caseA(int arr[], int *n, int k, int *comps, int *assignment) {
    *comps = 0;
    *assignment = 0;

    int i = 0;
    while (i < *n) {
        (*comps)++;
        if (arr[i] >= k) {
            deleteArray(arr, n, i, assignment);
        } else {
            i++;
        }
    }
}

// CASE B: sort + xóa một lần bằng cách cắt mảng
void caseB(int arr[], int *n, int k, int *comps, int *assignment) {
    *comps = 0;
    *assignment = 0;

    sort(arr, *n, comps, assignment);
    int pos = *n;
    for (int i = 0; i < *n; i++) {
        (*comps)++;
        if (arr[i] >= k) {
            pos = i;
            break;
        }
    }
    *n = pos;
}

int main() {
    int arr[100], arrA[100], arrB[100];
    int k = 2016, n = 100;
    int compsA, compsB, assignA, assignB;

    srand(time(NULL));

    initializeArray(arr, n, 1998, 2023);

    for (int i = 0; i < n; i++) {
        arrA[i] = arr[i];
        arrB[i] = arr[i];
    }

    caseA(arrA, &n, k, &compsA, &assignA);
    caseB(arrB, &n,k, &compsB, &assignB);

    printf("=== CASE A ===\n");
    printf("Comparisons: %d\n", compsA);
    printf("Assignments: %d\n\n", assignA);

    printf("=== CASE B ===\n");
    printf("Comparisons: %d\n", compsB);
    printf("Assignments: %d\n\n", assignB);

    printf("Nhan xet:\n");
    printf("CaseA phai xoa va dich mang nhieu lan nen ton nhieu phep gan.\n");
    printf("CaseB sap xep truoc nhung chi xoa mot lan nen hieu qua hon voi du lieu lon.\n");

    return 0;
}

//BÀi 6 - maximum product
#include <stdio.h>
#include <stdlib.h>
#include <time.h>

// Khởi tạo mảng ngẫu nhiên
void initializeArray(int arr[], int n, int min, int max) {
    for (int i = 0; i < n; i++)
        arr[i] = min + rand() % (max - min + 1);
}

// Hàm tìm tích lớn nhất + đếm steps
int maxProduct(int arr[], int n, int *steps_mul, int *steps_assign)
{
    *steps_mul = 0;
    *steps_assign = 0;

    int max = arr[0] * arr[1];
    (*steps_mul)++; // 1 phép nhân đầu tiên

    for (int i = 0; i < n - 1; i++) {
        for (int j = i + 1; j < n; j++) {

            int product = arr[i] * arr[j];
            (*steps_mul)++;

            if (product > max) {
                max = product;
                (*steps_assign)++;
            }
        }
    }

    return max;
}

int main() {
    int arr[1000];
    int n = 4;   // yêu cầu đề bài
    int steps_mul, steps_assign;

    int k[] = {100, 1000, 10000};

    srand((unsigned)time(NULL));

    // Số assignment tối đa của N = 4 là 5 nên dùng freTable[10] là đủ
    for (int i = 0; i < 3; i++) {

        int freTable[10] = {0};

        for (int j = 0; j < k[i]; j++) {

            initializeArray(arr, n, 1, 100);

            maxProduct(arr, n, &steps_mul, &steps_assign);

            freTable[steps_assign]++;
        }

        printf("k = %d; Mean = ", k[i]);

        float sum = 0;
        for (int l = 0; l < 10; l++)
            sum += (float)l * freTable[l];

        sum /= k[i];

        printf("%f\n", sum);
    }

    return 0;
}

//Bài 7 - Số tăng dần

#include <stdio.h>

// Hàm kiểm tra chữ số tăng dần và đếm số phép so sánh
int isIncreasing(int n, int *comps)
{
    *comps = 0;

    int d4 = n % 10; n /= 10;
    int d3 = n % 10; n /= 10;
    int d2 = n % 10; n /= 10;
    int d1 = n % 10;

    // d1 d2 d3 d4 là 4 chữ số

    (*comps)++;
    if (d1 > d2) return 0;

    (*comps)++;
    if (d2 > d3) return 0;

    (*comps)++;
    if (d3 > d4) return 0;

    return 1;
}

int main() {
    int comps;
    long long totalComps = 0;
    int count = 0;

    for (int n = 1000; n <= 9999; n++) {

        isIncreasing(n, &comps);

        totalComps += comps;
        count++;
    }

    double mean = (double)totalComps / count;

    printf("Tong so so da kiem tra: %d\n", count);
    printf("Tong so phep so sanh: %lld\n", totalComps);
    printf("So phep so sanh trung binh: %.6f\n", mean);

    return 0;
}
//Bài 8
#include <stdio.h>

void f(int a[], int n, int *comps, int *shifts, int *assigns) {

    *comps = 0;
    *shifts = 0;
    *assigns = 0;

    for (int d = n/2; d > 0; d /= 2) {

        for (int i = d; i < n; i++) {

            int t = a[i];   
            int j;

            for (j = i; j >= d; j -= d) {

                (*comps)++;    // so sánh a[j-d] > t

                if (a[j-d] > t) {
                    a[j] = a[j-d];   // SHIFT
                    (*shifts)++;
                } else {
                    break;
                }
            }

            a[j] = t;   
            (*assigns)++;
        }
    }
}

int main() {

    int a[] = {34, 29, 10, 3, 2, 44, 88, 1, 21, 12,
               3, 18, 21, 23, 37, 17, 8, 9, 4, 0};

    int n = 20;
    int comps, shifts, assigns;

    f(a, n, &comps, &shifts, &assigns);

    printf("So phep so sanh (comps)  = %d\n", comps);
    printf("So phep shift (a[j]=a[j-d]) = %d\n", shifts);
    printf("So phep assign (a[j]=t)   = %d\n", assigns);

    return 0;
}


//Bài 8
// Online C compiler to run C program online
#include <stdio.h>
void f(int a[], int n, int *assignments, int *shifts) {
  for (int d = n/2; d > 0; d /= 2) {
    for (int i = d; i < n; i++) {
      int t = a[i];
      (*assignments)++;
      int j;
      for (j = i; j >=d && a[j-d] > t; j -= d) {
          (*shifts)++;
          (*assignments)++;
        a[j] = a[j-d];
      }
      a[j] = t;
    (*assignments)++;
    }
  }
}

int main() {
  int a[] = {34, 29, 10, 3, 2, 44, 88, 1, 21, 12, 3, 18, 21, 23, 37, 17, 8, 9, 4, 0};
  int n = 20;
  int assignments=0, shifts=0;
  f(a,n, &assignments, &shifts);
  printf("Mảng sau sắp xếp:\n");
  for(int i=0; i<n; i++){
      printf("%d  ", a[i]);
  }
  printf("\nassignmentsignments: %d", assignments);
  printf("\nShifts: %d", shifts);
  return 0;
}




// Ôn tập 2
#include <stdio.h>
#include <stdlib.h>
#include <time.h>
void initializeArray(int arr[], int n, int min, int max) {
    for (int i = 0; i < n; i++){
        arr[i] = min + rand()%(max-min+1)/4;
        min = arr[i];
    }
}

void oddLeft(int arr[], int n, int *comps, int *shifts){
    (*comps) =0;
    (*shifts) = 0;
     for(int i=1; i<n; i++){
          int j = i;
          (*comps)++;
          if (arr[j]%2 != 0){
              int temp = arr[j];
              while(arr[j-1] %2 == 0 && j>0){
                  (*shifts)++;
                  (*comps)++;
                   arr[j] = arr[j - 1];
                j--;
            }
            arr[j] = temp;
          }
     }
}

int main(){
    int n = 10, comps, shifts;
    int arr[100];
    int k = 10000;
   //Sinh mang ngau nhien
    time_t t;
    srand((unsigned)time(&t));
    
   int freqTableShift[30] = {};
    int freqTableComps[50] = {};
     for (int j = 0; j < k; j++){
            initializeArray(arr, n, -100, 100);
            oddLeft(arr, n, &comps, &shifts);
            freqTableShift[shifts]++;
            freqTableComps[comps]++;
        }
    printf("\n Shifts     Freq\n");
    float shift_fre = 0, comps_fre =0;
    for (int i =0; i<30; i++){
        printf("%d     %d\n", i, freqTableShift[i]);
        shift_fre += freqTableShift[i]*i;
    }
    printf("\nMean Shift = %.2f", (float) shift_fre/k);
     printf("\n Comps     Freq\n");
    for (int i =0; i<50; i++){
        printf("%d     %d\n", i, freqTableComps[i]);
        comps_fre += freqTableComps[i]*i;
    }
   printf("\nMean Comps = %.2f", (float) comps_fre/k);
   printf("\nĐộ phức tạp của thuật toán theo Big O: O(n^2)");
    return 0;
    
}

// Ôn tập 2
#include <stdio.h>
#include <stdlib.h>
#include <time.h>
void initializeArray(int arr[], int n, int min, int max) {
    for (int i = 0; i < n; i++){
        arr[i] = min + rand()%(max-min+1)/4;
        min = arr[i];
    }
}

void oddLeft(int arr[], int n, int *comps, int *shifts){
    (*comps) =0;
    (*shifts) = 0;
     for(int i=1; i<n; i++){
          int j = i;
          (*comps)++;
          if (arr[j]%2 != 0){
              int temp = arr[j];
              while(arr[j-1] %2 == 0 && j>0){
                  (*shifts)++;
                  (*comps)++;
                   arr[j] = arr[j - 1];
                j--;
            }
            arr[j] = temp;
          }
     }
}

int main(){
    int n = 10, comps, shifts;
    int arr[100];
    int k = 10000;
   //Sinh mang ngau nhien
    time_t t;
    srand((unsigned)time(&t));
    
   int freqTableShift[30] = {};
    int freqTableComps[50] = {};
     for (int j = 0; j < k; j++){
            initializeArray(arr, n, -100, 100);
            oddLeft(arr, n, &comps, &shifts);
            freqTableShift[shifts]++;
            freqTableComps[comps]++;
        }
    printf("\n Shifts     Freq\n");
    float shift_fre = 0, comps_fre =0;
    for (int i =0; i<30; i++){
        printf("%d     %d\n", i, freqTableShift[i]);
        shift_fre += freqTableShift[i]*i;
    }
    printf("\nMean Shift = %.2f", (float) shift_fre/k);
     printf("\n Comps     Freq\n");
    for (int i =0; i<50; i++){
        printf("%d     %d\n", i, freqTableComps[i]);
        comps_fre += freqTableComps[i]*i;
    }
   printf("\nMean Comps = %.2f", (float) comps_fre/k);
   printf("\nĐộ phức tạp của thuật toán theo Big O: O(n^2)");
    return 0;
    
}


// Brute Force
#include <stdio.h>
#include <stdlib.h>
#include <time.h>

void initializeArray(int arr[], int n, int min, int max){
    for(int i = 0; i < n; i++){
        arr[i] = min + rand() % (max - min + 1);
    }
}

int maxSubArrayBruteForce(int arr[], int n, long long *comparisons, long long *assignments)
{
    *comparisons = *assignments = 0;

    int maxSum = arr[0]; 
    (*assignments)++;

    for(int i = 0; i < n; i++){
        int currSum = 0;

        for(int j = i; j < n; j++){
            currSum += arr[j];

            (*comparisons)++;
            if(currSum > maxSum){
                maxSum = currSum;
                (*assignments)++;
            }
        }
    }
    return maxSum;
}

int main(){
    srand(time(NULL));

    int n = 10;   // độ lớn mảng mỗi lần test
    int k = 1000; // sẽ thay 100, 1000, 10000 theo yêu cầu

    int arr[100];
    long long comparisons, assignments;

    long long freqComp[200000] = {0};
    long long freqAssign[200000] = {0};

    for(int t = 0; t < k; t++){
        initializeArray(arr, n, -50, 50);

        maxSubArrayBruteForce(arr, n, &comparisons, &assignments);

        if(comparisons < 200000) freqComp[comparisons]++;
        if(assignments < 200000) freqAssign[assignments]++;
    }

    printf("\n=== k = %d ===", k);
    long double sumC = 0, sumA = 0;

    for(int i = 0; i < 200000; i++){
        sumC += (long double) i * freqComp[i];
        sumA += (long double) i * freqAssign[i];
    }

    printf("\nMean comparisons = %Lf", sumC / k);
    printf("\nMean assignments = %Lf\n", sumA / k);

    printf("\nBig-O time complexity = O(n^3)\n");

    return 0;
}

// In 2 bảng riêng
#include <stdio.h>
#include <stdlib.h>
#include <time.h>

void initializeArray(int arr[], int n, int min, int max) {
    for (int i = 0; i < n; i++)
        arr[i] = min + rand() % (max - min + 1);
}

int maxProduct(int arr[], int n, int *steps_multi, int *steps_assign){
    (*steps_multi) = 0;
    (*steps_assign) = 0;

    int maxProduct = arr[0] * arr[1];  

    for (int i = 0; i < n - 1; i++){
        for (int j = i + 1; j < n; j++){
            (*steps_multi)++;
            int product = arr[i] * arr[j];
            if (product > maxProduct){
                (*steps_assign)++;
                maxProduct = product;
            }
        }
    }
    return maxProduct;
}

int main() {
    int n = 4, steps_multi, steps_assign;
    int arr[100];
    int k = 10000;

    srand(time(NULL));

    // Ý 1: in mảng và số steps
    initializeArray(arr, n, 1, 1000);
    printf("Mang ngau nhien: ");
    for (int i = 0; i < n; i++)
        printf("%d  ", arr[i]);

    int max = maxProduct(arr, n, &steps_multi, &steps_assign);
    printf("\nSteps nhân (multi): %d", steps_multi);
    printf("\nSteps chính xác theo n: n*(n-1)/2");
    printf("\nTheo Big O: O(n^2)\n");

    // Ý 2: chạy 10.000 lần
    int freqAssign[10] = {0};
    int freqMulti[10] = {0};

    for (int j = 0; j < k; j++){
        initializeArray(arr, n, 1, 1000);
        maxProduct(arr, n, &steps_multi, &steps_assign);

        if (steps_assign < 10)
            freqAssign[steps_assign]++;

        if (steps_multi < 10)
            freqMulti[steps_multi]++;
    }

    // ================================
    // BẢNG 1: THỐNG KÊ ASSIGN
    // ================================
    printf("\n==============================\n");
    printf(" BANG TAN SUAT ASSIGN (max=product)\n");
    printf("==============================\n");
    printf("Assign   Frequency\n");

    float sum_assign = 0;
    for (int i = 0; i < 10; i++) {
        printf("%4d     %6d\n", i, freqAssign[i]);
        sum_assign += freqAssign[i] * i;
    }
    printf("Trung binh Assign = %.3f\n", sum_assign / k);

    // ================================
    // BẢNG 2: THỐNG KÊ MULTIPLY
    // ================================
    printf("\n==============================\n");
    printf(" BANG TAN SUAT MULTIPLY (arr[i]*arr[j])\n");
    printf("==============================\n");
    printf("Multi    Frequency\n");

    float sum_multi = 0;
    for (int i = 0; i < 10; i++) {
        printf("%4d     %6d\n", i, freqMulti[i]);
        sum_multi += freqMulti[i] * i;
    }
    printf("Trung binh Multi = %.3f\n", sum_multi / k);

    return 0;
}

// SINH MẢNG 2 CHIỀU NGẪU NHIÊN
void init2D(int a[][100], int rows, int cols, int min, int max){
    for (int i = 0; i < rows; i++) {
        for (int j = 0; j < cols; j++) {
            a[i][j] = min + rand() % (max - min + 1);
        }
    }
}

//BUBBLE SORT
#include <stdio.h>
#include <time.h>
#include <stdlib.h>

void bubbleSort(int arr[], int n, int *comps, int *swaps) {
    *swaps = 0;
    *comps = 0;
    for (int i = 0; i<n-1; i++) {
        for (int j = i+1; j<n; j++) {
            (*comps)++;
            if (arr[i] > arr[j]) {
                int temp = arr[i];
                arr[i] = arr[j];
                arr[j] = temp;
                (*swaps)++;
            }
        }
    }
}

void inputArray(int arr[], int n) {
    for (int i = 0; i<n; i++){
        printf("arr[%d] = ", i);
        scanf("%d", &arr[i]);
    }
}

void initializeArray(int arr[], int n, int min, int max) {
    for (int i = 0; i<n; i++) {
        arr[i] = min + rand() % (max - min + 1);
    }
}

int main() {
    time_t t;
    srand((unsigned)time(&t));
    printf("\n");
    
    int arr[100];
    int n = 10;
    //printf("Size of array: ");
    //scanf("%d", &n);
    
    int k[] = {100,1000,10000};
    int swaps, comps;
    
    printf("        FREQUENCY TABLE       ");
    for (int i = 0; i<3; i++) {
        int compFre[51] = {0};
        int swapFre[51] = {0};
        float compSum = 0;
        float swapSum = 0;
        printf("\n-------------------------------\n");
        printf("k = %d\n", k[i]);
        printf("Observation\tComps\tSwaps\n");
        for (int j = 0; j<k[i]; j++) {
            initializeArray(arr, n, -200, 200);
            bubbleSort(arr, n, &comps, &swaps);
            
            compFre[comps]++;
            swapFre[swaps]++;
        }
        for (int i = 0; i<51; i++) {
                printf("%d\t\t%d\t%d\n", i, compFre[i], swapFre[i]);
                compSum += i*compFre[i];
                swapSum += i*swapFre[i];
        }
        compSum/=k[i];
        swapSum/=k[i];
        printf("Mean of comps: %.2f", compSum);
        printf("\nMean of swaps: %.2f", swapSum);
    }
    
    return 0;
}
